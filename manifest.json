{
  "manifest_version": 2,

  "name": "ConsoleTap",
  "description": "Insert JavaScript Libraries Into Your Console",
  "version": "0.1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "menu.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ]
}
